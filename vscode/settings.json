{
  "telemetry.enableTelemetry": false,
  "cSpell.language": "en,ru",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Community Material Theme High Contrast",
  "workbench.fontAliasing": "antialiased",
  "workbench.tips.enabled": false,
  "workbench.colorCustomizations": {
    "editor.findMatchBackground": "#8a7fff",
    "editor.findMatchBorder": "#8a7fff",
    "editor.findMatchHighlightBorder": "#8a7fff",
    "editorInfo.foreground": "#8a7fff",
    "editorError.foreground": "#ff8a7f",
    "editorWarning.foreground": "#f4ff7f",
    "editorHint.foreground": "#7fffca",
    "gitDecoration.addedResourceForeground": "#7fffca",
    "editorGutter.addedBackground": "#7fffca",
    "gitDecoration.modifiedResourceForeground": "#f4ff7f",
    "editorGutter.modifiedBackground": "#f4ff7f",
    "gitDecoration.deletedResourceForeground": "#ff8a7f",
    "editorGutter.deletedBackground": "#ff8a7f"
  },
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.tabCompletion": "on",
  "editor.wordWrap": "wordWrapColumn",
  "editor.wrappingIndent": "indent",
  "editor.formatOnSave": true,
  "editor.rulers": [60, 80],
  "[haskell]": {
    "editor.defaultFormatter": "haskell.haskell"
  },
  "haskell.formattingProvider": "stylish-haskell",
  "haskell.updateBehavior": "never-check",
  "python.pythonPath": "python3",
  "python.autoComplete.addBrackets": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pylintArgs": [
    // "--init-hook='import sys; sys.path.append(\"/usr/local/lib/python3.6/dist-packages\")'"
    "--load-plugins",
    "pylint_django"
  ],
  "vetur.validation.templateProps": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "Monoid",
    "fmap",
    "foldl",
    "foldr",
    "mappend",
    "mempty",
    "scanl"
  ],
  "haskell.serverExecutablePath": "/Users/worm2fed/.ghcup/bin/haskell-language-server-wrapper"
}
